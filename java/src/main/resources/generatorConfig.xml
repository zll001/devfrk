<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry
		location=".\libs\mysql-connector-java-6.0.6.jar" />
	<!--数据库驱动 -->
	<context id="mysql">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接地址账号密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/application?characterEncoding=utf8&amp;serverTimezone=UTC" userId="root"
			password="onceas">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!--生成Model类存放位置 -->
		<javaModelGenerator
			targetPackage="cn.abcsys.faas.application.core.orm.model"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成映射文件存放位置 -->
		<!-- <sqlMapGenerator
			targetPackage="cn.abcsys.faas.application.core.orm.dao"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator> -->
		<!--生成mapper类存放位置 -->
		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="cn.abcsys.faas.application.core.orm.mapper"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!--生成对应表及类名 -->

		<table tableName="application_events"
			domainObjectName="ApplicationEvent" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="project_core"
			domainObjectName="ApplicationProject" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="v2_application" domainObjectName="Application"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>

		<table tableName="v2_version"
			domainObjectName="ApplicationVersion" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="v2_image_group"
			domainObjectName="ApplicationController" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>

		<table tableName="v2_container"
			domainObjectName="ApplicationComponent" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>


		<table tableName="v2_pod" domainObjectName="ApplicationInstance"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>

	</context>
</generatorConfiguration>